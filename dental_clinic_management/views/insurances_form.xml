<?xml version="1.0"?>
<odoo>
    <data>
        <record id="insurance_form" model="ir.ui.view">
            <field name="name">Insurance form</field>
            <field name="model">insurance</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>

                        <group string="Insurance Info">
                            <group>
                                <field name="Insurance_number"/>
                                <field name="Insurance_type"/>
                                <field name="insurance_plan"/>
                                <field name="member_since_date"/>
                                <field name="extra_info"/>
                            </group>
                            <group>
                                <field name="owner"/>
                                <field name="Insurance_company"/>
                                <field name="category"/>
                                <field name="expiration_date"/>
                            </group>
                        </group>

                    </sheet>
                </form>
            </field>
        </record>
    </data>


</odoo>

